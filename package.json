{
  "name": "ollama-js",
  "version": "1.0.0",
  "description": "JS fetch wrapper for consuming the Ollama API in node and the browser",
  "repository": {
    "type":"git",
    "url":"https://github.com/dditlev/ollama-js.git"
  },
  "type": "module",
  "module": "dist/node/index.js",
  "exports": {
    ".": {
      "import": "./dist/node/index.js"
    },
    "./JSONstore": {
      "import": "./dist/node/json_store.js"
    },
    "./JSONparser": {
      "import": "./dist/node/json_parser.js"
    },
    "./browser": {
      "browser": "./dist/browser/index.js"
    },
    "./JSONstore/browser": {
      "browser": "./dist/browser/json_store.js"
    },
    "./JSONparser/browser": {
      "browser": "./dist/browser/json_parser.js"
    }
  },
  "browser": "./dist/browser/index.js",
  "types": "dist/node/types/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "node:test": "node examples/test/run_node.js"
  },
  "keywords": [
    "ollama",
    "ai",
    "llama2"
  ],
  "author": "Ditlev TÃ¸jner",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "node-fetch": "^3.0.0"
  }
}
